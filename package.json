	"dependencies": {
		"@types/jqueryui": "^1.12.17",
		"bootstrap": "^5.3.0",
		"dotenv": "^16.3.1",
		"jquery": "^3.7.0",
		"jquery-ui": "^1.13.2",
		"monaco-editor": "^0.39.0",
		"vite-plugin-static-copy": "^0.16.0"
	}
